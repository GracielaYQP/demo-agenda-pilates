{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:pages": "ng build --configuration production --base-href /agenda-clases-pilates/ --deploy-url /agenda-clases-pilates/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prep:pages": "node -e \"const fs=require('fs');fs.copyFileSync('dist/browser/index.html','dist/browser/404.html');fs.writeFileSync('dist/browser/.nojekyll','');\"",
    "deploy": "npm run build:pages && npm run prep:pages && gh-pages -d dist/browser -b gh-pages -m \"Deploy $(git rev-parse --short HEAD)\" --dotfiles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.14",
    "@angular/common": "18.2.14",
    "@angular/compiler": "18.2.14",
    "@angular/core": "18.2.14",
    "@angular/forms": "18.2.14",
    "@angular/platform-browser": "18.2.14",
    "@angular/platform-browser-dynamic": "18.2.14",
    "@angular/router": "18.2.14",
    "@angular/service-worker": "18.2.14",
    "bootstrap": "^5.3.7",
    "date-fns-tz": "^3.2.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.22.2",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "18.2.14",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}
